{
  "extends": [
    "plugin:unicorn/recommended",
    "next/core-web-vitals",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "no-console": "error",
    "unicorn/prevent-abbreviations": [
      "error",
      {
        "allowList": {
          "getInitialProps": true,
          "getStaticProps": true
        }
      }
    ]
  }
}
